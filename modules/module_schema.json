{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "$schema": {
      "const": "../module_schema.json"
    },
    "name": {
      "pattern": "^[a-z|0-9|_]+$"
    },
    "version": {
      "pattern": "^[0-9|\\.]+$"
    }
  },
  "additionalProperties": false,
  "required": [
    "$schema",
    "name",
    "version"
  ]
}